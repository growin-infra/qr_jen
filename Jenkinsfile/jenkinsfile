node {
    def mvnHome
	stage ('whoami') {
	    sh "whoami"
	}
	stage ('check environment') {
	    sh "M2_HOME=/usr/local/maven"
	    sh "export PATH=${M2_HOME}/bin:${PATH}"
	    sh "echo $PATH"
	}
	stage ('check maven') {
	    mvnHome = tool 'M2'
	    sh "'${mvnHome}/bin/mvn' -version"
	    sh "mvn -version"
	}
    dir ('/downloads/maven/qr_jen') {
	    stage ('git clone') {
            git 'https://growin-infra:rmfhdnls1234@github.com/growin-infra/qr_jen.git'
        }
	}
}