node {
    def mvnHome
	stage ('whoami') {
	    sh "whoami"
	}
	stage ('check environment') {
	    sh "echo $PATH"
	    sh "echo $M2_HOEM"
	    sh "echo $JAVA_HOME"
	}
	stage ('check maven') {
	    mvnHome = tool 'M2'
	    sh "'${mvnHome}/bin/mvn' -version"
	    sh "mvn -version"
	}
}