node {
	stage ('ls test') {
	    sh "ls /app/growin/was/"
	    sh "whoami"
	}
    dir ('/svc/growin/was/tomcat/bin/') {
	    stage ('tomcat restart') {
		    sh "./shutdown.sh"
			sh "sleep 5"
		    sh "./startup.sh"
		}
	}
    stage ('Deploy') {
        sh "echo 'Deply tomcat'"
    }
}